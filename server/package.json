{
  "name": "pi_manager_server",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "build": "npx tsc",
    "eslint": "npx eslint .",
    "prod": "cross-env NODE_ENV=production node ./dist/src/server.js",
    "nodemon_database": "cross-env NODE_ENV=dev_database concurrently \"tsc -w\" \"nodemon ./dist/src/server.js\"",
    "nodemon_file": "cross-env NODE_ENV=dev_file concurrently \"tsc -w\" \"nodemon ./dist/src/server.js\"",
    "dev_database": "cross-env NODE_ENV=dev_database node ./dist/src/server.js",
    "dev_file": "cross-env NODE_ENV=dev_file node ./dist/src/server.js",
    "test_domain": "npx jest --runInBand --coverage --testPathPattern=./__tests__/domain",
    "test_infrastructure": "npx jest --runInBand  --silent=false --coverage --testPathPattern=./__tests__/infrastructure",
    "test_api_database": "cross-env NODE_ENV=test_api_database npx jest --silent=true --runInBand --coverage  --testPathPattern=./__tests__/apiTests",
    "test_api_file": "cross-env NODE_ENV=test_api_file npx jest --silent=true --runInBand --coverage  --testPathPattern=./__tests__/apiTests",
    "test_all_file": "cross-env NODE_ENV=test_api_file npx jest --silent=true --runInBand --coverage  --testPathPattern=./__tests__/",
    "test_all_database": "cross-env NODE_ENV=test_api_database npx jest --silent=true --runInBand --testPathPattern=./__tests__/"
  },
  "author": "Konrad Winnicki",
  "license": "ISC",
